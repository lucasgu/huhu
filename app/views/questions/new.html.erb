<h1>New Question</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    



<%= form_for(@question) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
     <%= f.label :content %>
     <%= f.text_field :content %>

    

    <%= f.submit"Create", class: "btn btn-large btn-primary"  %> 
<% end %>
  </div>
</div>
